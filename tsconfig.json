{
    "compilerOptions": {
      "outDir": "./build/",

      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictBindCallApply": true,
      "alwaysStrict": true,

      "noEmitOnError": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true,
      "moduleResolution": "node"
    }
  }