{
  "name": "photo-portfolio",
  "version": "0.3.0",
  "description": "React Photo Portfolio",
  "main": "index.html",
  "dependencies": {
    "axios": "^0.19.0",
    "mobx": "^5.15.0",
    "mobx-react": "^5.4.4",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.1",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^0.28.11",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.9.4",
    "react-test-renderer": "^16.6.3",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^6.0.7",
    "style-loader": "^1.0.0",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.2"
  },
  "scripts": {
    "test": "jest --updateSnapshot",
    "dev": "webpack -d --watch",
    "build": "webpack -p && cp src/index.html build/index.html",
    "start": "webpack-dev-server --open"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  },
  "author": "Dave Feucht",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/davefeucht/photo-portfolio.git"
  }
}
